    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.97.0",
    "constructs": "^10.2.70",
    "execa": "^5.1.1",
    "source-map-support": "^0.5.21"
  }
